{
  "expo": {
    "name": "CellWise",
    "slug": "CellWise",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#EB4335"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nnadozi.CellWise"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#EB4335"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.nnadozi.CellWise"
    },
    "web": {
      "favicon": "./src/assets/images/config/logo.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "src/assets/fonts/Rubik-Bold.ttf",
            "src/assets/fonts/Rubik-Medium.ttf",
            "src/assets/fonts/Rubik-Regular.ttf",
            "src/assets/fonts/Rubik-SemiBold.ttf"
          ]
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "extraProguardRules": "-keep public class com.horcrux.svg.** {*;}",
            "allowBackup": false
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "cd2bae19-0f52-4611-a5ad-f7d034f6e114"
      }
    }
  }
}
